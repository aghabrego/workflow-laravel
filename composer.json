{
  "name": "weirdo/laravel-workflow",
  "description": "Integerate Symfony Workflow component into Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "Angel Hidalgo",
      "email": "aghabrego@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "keywords": [
    "workflow",
    "symfony",
    "laravel"
  ],
  "require": {
    "php": "^8.0|^8.1",
    "symfony/workflow": "^5.4|^6.0",
    "symfony/process": "^5.4|^6.0",
    "symfony/var-dumper": "^5.4|^6.0",
    "symfony/event-dispatcher-contracts": "^2.5",
    "illuminate/console": "8.*|9.*",
    "illuminate/support": "8.*|9.*",
    "illuminate/contracts": "8.*|9.*"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^9.3@dev",
    "fakerphp/faker": "^1.13",
    "orchestra/testbench": "^7.0.0"
  },
  "autoload": {
    "psr-4": {
      "Weirdo\\LaravelWorkflow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Weirdo\\LaravelWorkflow\\WorkflowServiceProvider"
      ],
      "aliases": {
        "Workflow": "Weirdo\\LaravelWorkflow\\Facades\\WorkflowFacade"
      }
    }
  }
}