{
    "name": "weirdo/laravel-workflow",
    "description": "Integerate Symfony Workflow component into Laravel.",
    "keywords": [
        "workflow",
        "symfony",
        "laravel",
        "laravel7",
        "laravel8"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "symfony/workflow": "^5.4",
        "symfony/process": "^5.4",
        "symfony/event-dispatcher-contracts": "^2.4",
        "illuminate/console": "^7.1|^8.0",
        "illuminate/support": "^7.1|^8.0",
        "illuminate/contracts": "^7.1|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^5.0",
        "fakerphp/faker": "^1.13",
        "orchestra/testbench": "^5.2|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Weirdo\\LaravelWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weirdo\\LaravelWorkflow\\WorkflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "Weirdo\\LaravelWorkflow\\Facades\\WorkflowFacade"
            }
        }
    }
}